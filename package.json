{
  "name": "@shariffff/wp-starter-plugin",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://github.com/shariffff/wp-starter-plugin",
  "scripts": {
    "build": "wp-scripts build src/settings.js --output-path=build",
    "start": "wp-scripts start src/settings.js --output-path=build",
    "hot": "wp-scripts start src/settings.js --output-path=build --hot --allowed-hosts all",
    "env": "wp-env"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@wordpress/env": "^5.13.0",
    "@wordpress/scripts": "25.5.1"
  },
  "browserslist": "last 2 versions, Firefox ESR, not dead, not op_mini all, not ie <= 11"
}